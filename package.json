{
    "name": "vite-plugin-tanstack-router-paths",
    "version": "1.0.0",
    "description": "A Vite plugin that automatically generates type-safe navigation helpers for TanStack Router routes",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "vite",
        "plugin",
        "tanstack",
        "router",
        "typescript",
        "navigation",
        "routes",
        "type-safe"
    ],
    "author": "Quintard Clement <clement@quintard.me>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/clement83/vite-plugin-tanstack-router-paths.git"
    },
    "bugs": {
        "url": "https://github.com/clement83/vite-plugin-tanstack-router-paths/issues"
    },
    "homepage": "https://github.com/clement83/vite-plugin-tanstack-router-paths#readme",
    "peerDependencies": {
        "vite": "^2.0.0 || ^3.0.0 || ^4.0.0 || ^5.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0",
        "vite": "^6.0.0"
    },
    "dependencies": {
        "chokidar": "^3.5.3"
    },
    "engines": {
        "node": ">=16"
    }
}